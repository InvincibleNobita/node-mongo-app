<%- include ('./layout.ejs') %>

    <div class="app">
        <div class="home">
            <div class="intro-header">
                <h2>Welcome to the dashboard</h2>
                <h4>- Database data in Tabular manner</h4>
            </div>
        </div>
    </div>

    <table class="content-table">
        <thead>
            <th>Team Name</th>
            <th>Full Name</th>
            <th>Email</th>
            <th>Number</th>
            <th>City</th>
            <th>Url</th>
        </thead>
        
        
            
        <tbody>
            <% for (user of users) { %>
                <% if (user) { %>
                <tr>
                    <td><%= user.team_name %></td>
                    <td><%= user.full_name %></td>
                    <td><%= user.email %></td>
                    <td><%= user.number %></td>
                    <td><%= user.city %></td>
                    <td><%= user.url %></td>
                </tr>
                <% } else { %>
                        <h3>Loading.....</h3>
                <%}%>
            <% } %>
        </tbody>
    </table>
                            
                            
                    
                    
